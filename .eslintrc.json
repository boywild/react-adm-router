{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": ["react", "jsx-a11y", "import"],
    "rules": {
        "no-console": 0,
        "react/forbid-prop-types": 0,
        "react/prop-types": [2, { "ignore": ["children"] }]
    },
    "globals": {
        "document": true,
        "window": true
    },
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    }
}
